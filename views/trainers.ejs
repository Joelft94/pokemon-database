<!DOCTYPE html>
<html>
<head>
    <title>Trainers</title>
</head>
<body>
    <h1>Trainers</h1>
    <ul>
        <% trainers.forEach(trainer => { %>
            <li><a href="/trainers/<%= trainer.id %>"><%= trainer.name %></a></li>
        <% }); %>
    </ul>
    <a href="/trainers/new">Add New Trainer</a>
</body>
</html>
